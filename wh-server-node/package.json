{
  "name": "wh-server-node",
  "version": "1.0.0",
  "description": "Aycron challenge Server",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:integration": "jest --testPathPattern=__test__",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "build": "tsc -p .",
    "prod": "node ./dist/index.js",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "migrate:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/config/typeorm.ts",
    "migrate:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/config/typeorm.ts"
  },
  "keywords": [
    "Aycron",
    "test"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-config": "^20.10.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dateformat": "^5.0.3",
    "dotenv": "^16.0.1",
    "email-validator": "^2.0.4",
    "express": "^4.18.1",
    "express-jsdoc-swagger": "^1.8.0",
    "http-terminator": "^3.2.0",
    "jsonwebtoken": "^9.0.0",
    "md5-typescript": "^1.0.5",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "typeorm": "^0.3.7"
  },
  "author": "Juan Andr√©s Carmena",
  "license": "ISC"
}
